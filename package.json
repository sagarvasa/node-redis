{
  "name": "node-redis",
  "version": "1.0.0",
  "description": "OTP flow using node.js & redis",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "docker:build": "docker build -t node-ts-mongoose .",
    "docker:run": "docker run -p 3000:3000 -d node-redis",
    "prestart": "npm run build",
    "start": "node ."
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagarvasa/node-redis.git"
  },
  "keywords": [
    "Node",
    "Redis",
    "OTP",
    "Caching"
  ],
  "author": "Sagar Vasa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sagarvasa/node-redis/issues"
  },
  "homepage": "https://github.com/sagarvasa/node-redis#readme",
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "http-errors": "1.8.0",
    "shortid": "2.2.16",
    "util": "0.12.3",
    "winston": "3.3.3",
    "redis": "3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/http-errors": "^1.8.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "@types/shortid": "0.0.29",
    "typescript": "^4.1.3",
    "prettier": "^2.2.1",
    "@types/redis": "^2.8.28"
  }
}
